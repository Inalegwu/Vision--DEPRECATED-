{
	"$schema": "https://biomejs.dev/schemas/1.4.1/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"correctness":{
				"noUnusedLabels":"warn",
				"noUnusedVariables":"error",
				"noUnreachable":"error"
			}
		}
	},
	"formatter":{
		"indentStyle": "tab",
		"indentSize": 6
	}
}
