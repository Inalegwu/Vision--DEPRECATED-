{
	"$schema": "https://biomejs.dev/schemas/1.4.1/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"correctness":{
				"noUnusedLabels":"warn",
				"noUnusedVariables":"error",
				"noUnreachable":"error"
			}
		}
	},
	"formatter":{
		"indentStyle": "space",
		"lineWidth": 80,
		"enabled": true,
		"lineEnding": "crlf",
		"formatWithErrors": true,
		"indentWidth": 2
	}
}
